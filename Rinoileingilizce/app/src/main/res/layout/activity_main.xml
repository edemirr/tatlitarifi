<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/SayılarMain"
        android:layout_width="0dp"
        android:layout_height="127dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="142dp"
        android:layout_marginRight="142dp"
        android:onClick="sayilaragit"
        android:src="@drawable/sayilarson"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/HarflerMain"
        app:layout_constraintTop_toBottomOf="@+id/baslikRino"
        tools:ignore="MissingConstraints,OnClick" />

    <ImageView
        android:id="@+id/FoodDrinkMain"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginTop="237dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="14dp"
        android:onClick="meyveleregit"
        android:src="@drawable/fooddrink"
        app:layout_constraintBottom_toTopOf="@+id/animalsMain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,OnClick"
        tools:layout_width="120dp" />

    <ImageView
        android:id="@+id/animalsMain"
        android:layout_width="123dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="213dp"
        android:onClick="hayvanlaragit"
        android:src="@drawable/animals"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FoodDrinkMain"
        tools:ignore="MissingConstraints,OnClick" />

    <ImageView
        android:id="@+id/colorsMain"
        android:layout_width="123dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:onClick="renkleregit"
        android:src="@drawable/colours"
        app:layout_constraintBottom_toTopOf="@+id/bodyMain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HarflerMain"
        tools:ignore="MissingConstraints,OnClick" />

    <ImageView
        android:id="@+id/bodyMain"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginBottom="71dp"
        android:onClick="vucudagit"
        android:src="@drawable/body"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colorsMain"
        tools:ignore="MissingConstraints,OnClick" />

    <ImageView
        android:id="@+id/baslikRino"
        android:layout_width="157dp"
        android:layout_height="0dp"
        android:layout_marginStart="114dp"
        android:layout_marginLeft="114dp"
        android:layout_marginBottom="71dp"
        android:src="@drawable/ustllooggo"
        app:layout_constraintBottom_toTopOf="@+id/HarflerMain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/HarflerMain"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="14dp"
        android:onClick="harfleregit"
        android:src="@drawable/leterslast"
        app:layout_constraintBottom_toTopOf="@+id/colorsMain"
        app:layout_constraintEnd_toStartOf="@+id/SayılarMain"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/baslikRino"
        tools:ignore="MissingConstraints,OnClick" />

    <ImageView
        android:id="@+id/rinoMain"
        android:layout_width="167dp"
        android:layout_height="0dp"
        android:layout_marginTop="11dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/colorsMain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SayılarMain"
        app:srcCompat="@drawable/llooggoo" />


</android.support.constraint.ConstraintLayout>